    "lessc": "^1.0.2",
    "melody-idom": "^1.7.5",
    "melody-runtime": "^1.7.5",
    "prettier": "^2.1.1",
    "prettier-plugin-twig-melody": "^0.4.6",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {}
}
